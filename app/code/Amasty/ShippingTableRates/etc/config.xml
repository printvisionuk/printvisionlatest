<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <amstrates>
                <active>0</active>
                <title>Shipping Table Rates</title>
                <sort_order>100</sort_order>
                <sallowspecific>0</sallowspecific>
                <specificerrmsg>This shipping method is currently unavailable. If you would like to ship using this shipping method, please contact us.</specificerrmsg>
                <allow_promo>0</allow_promo>
                <numeric_zip>1</numeric_zip>
                <ignore_virtual>1</ignore_virtual>
                <after_discount>0</after_discount>
                <including_tax>0</including_tax>
                <dont_split>0</dont_split>
                <showmethod>0</showmethod>
                <configurable_child>0</configurable_child>
                <bundle_child>0</bundle_child>
                <model>Amasty\ShippingTableRates\Model\Carrier\Table</model>
                <shipping_factor>5000</shipping_factor>
                <batch_duplicate_size>1000</batch_duplicate_size>
                <batch_duplicate_method_name_postfix>Duplicated</batch_duplicate_method_name_postfix>
                <status_for_duplicate_method>0</status_for_duplicate_method>
                <msi_algorithm>priority</msi_algorithm>
            </amstrates>
        </carriers>
    </default>
</config>
